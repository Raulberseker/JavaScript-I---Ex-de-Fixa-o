let num1 = 7;
let num2 = 6;
let num3 = 5;




let maior = num1;
let menor = num1;


//verificar o maior número
if (num2 > maior) {
  maior = num2;
}


if (num3 > maior) {
  maior = num3;
}


//verificar o menor número
if (num2 < menor) {
  menor = num2;
}


if (num3 < menor) {
  menor = num3;
}

// Impressão do maior e menor número
console.log("O maior número é: " + maior);
console.log("O menor número é: " + menor);
